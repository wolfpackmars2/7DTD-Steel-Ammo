		<item name="AP 9mm SC Ammo" count="10,40" loot_prob_template="T3"/>
		<item name="HP 9mm SC Ammo" count="7,30" loot_prob_template="T3"/>
		<item name="AP 44 Magnum SC Ammo" count="4,39" loot_prob_template="T3"/>
